		m00 = f / A, m01 = 0,  m02 = 0,                 m03 = 0
		m10 = 0,     m11 = f,  m12 = 0,                 m13 = 0
		m20 = 0,     m21 = 0,  m22 = (F + N) * nf,      m23 = -1
		m30 = 0,     m31 = 0,  m32 = (2 * F * N) * nf,  m33 = 0
	}

